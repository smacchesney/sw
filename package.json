{
  "name": "storywink-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:worker": "tsc --project tsconfig.worker.json --listEmittedFiles",
    "start": "next start",
    "lint": "next lint --fix",
    "format": "prettier --write .",
    "check-types": "tsc --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "next lint",
    "check-all": "npm run check-types && npm run check-format && npm run check-lint",
    "db:start": "docker compose up -d",
    "db:stop": "docker compose down",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "worker:dev:story": "ts-node --project tsconfig.worker.json -r tsconfig-paths/register src/queues/workers/story-generation.worker.ts",
    "worker:dev:illustration": "ts-node --project tsconfig.worker.json -r tsconfig-paths/register src/queues/workers/illustration-generation.worker.ts",
    "dev:all": "concurrently --names \"NEXT,STORY,ILLUS\" -c \"bgBlue.bold,bgMagenta.bold,bgGreen.bold\" \"npm:dev\" \"npm:worker:dev:story\" \"npm:worker:dev:illustration\"",
    "list": "node scripts/dev.js list",
    "generate": "node scripts/dev.js generate",
    "parse-prd": "node scripts/dev.js parse-prd"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@clerk/nextjs": "^6.16.0",
    "@dnd-kit/core": "^6.3.1",
    "@google/genai": "^0.9.0",
    "@prisma/client": "^6.6.0",
    "@radix-ui/react-alert-dialog": "^1.1.7",
    "@radix-ui/react-aspect-ratio": "^1.1.3",
    "@radix-ui/react-dropdown-menu": "^2.1.7",
    "@radix-ui/react-label": "^2.1.3",
    "@radix-ui/react-progress": "^1.1.4",
    "@radix-ui/react-radio-group": "^1.2.4",
    "@radix-ui/react-select": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-switch": "^1.1.4",
    "@radix-ui/react-tooltip": "^1.2.0",
    "boxen": "^8.0.1",
    "bullmq": "^5.48.1",
    "chalk": "^4.1.2",
    "class-variance-authority": "^0.7.1",
    "cli-table3": "^0.6.5",
    "cloudinary": "^2.6.0",
    "clsx": "^2.1.1",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "fastmcp": "^1.20.5",
    "figlet": "^1.8.0",
    "fuse.js": "^7.0.0",
    "gradient-string": "^3.0.0",
    "helmet": "^8.1.0",
    "inquirer": "^12.5.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^10.2.0",
    "lucide-react": "^0.488.0",
    "next": "15.3.0",
    "next-themes": "^0.4.6",
    "openai": "^4.93.0",
    "ora": "^8.2.0",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "puppeteer": "^24.7.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-resizable-panels": "^2.1.7",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.2.0",
    "tw-animate-css": "^1.2.5",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.1.2",
    "eslint": "^9",
    "eslint-config-next": "15.3.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-import-resolver-typescript": "^4.3.3",
    "eslint-plugin-import": "^2.31.0",
    "module-alias": "^2.2.3",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "prisma": "^6.6.0",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5"
  },
  "type": "module"
}
